{
  "version": 2,
  "builds": [
    {
      "src": "Backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/vite.config.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
